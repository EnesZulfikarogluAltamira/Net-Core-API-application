{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200/"
  },
  "TokenOptions": {
    "Audience": "www.altamira.com",
    "Issuer": "www.altamira.com",
    "AccessTokenExpiration": 60,
    "SecurityKey": "AltamiraSecurityKey"
  },

  "ConsulConfig": {
    "Address": "http://localhost:8500"
  },

  "AllowedHosts": "*"
}
